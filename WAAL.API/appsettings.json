    "CloudName": "example",
    "ApiKey": "example",
    "ApiSecret": "example"
  },
  "Authentication": {
    "Google": {
      "ClientId": "example",
      "ClientSecret": "example"
    },
    "Facebook": {
      "AppId": "example",
      "AppSecret": "example"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
